{  
   "$id":"http://gerdi-project.eu/schema/v1",
   "$schema":"http://json-schema.org/draft-07/schema",
   "type":"object",
   "definitions":{  

   },
   "properties":{  
      "identifier":{  
         "type":"object",
         "properties":{  
            "identifierType":{  
               "type":"string",
               "title":"The type of the identifier.",
               "examples":[  
                  "DOI"
               ]
            },
            "value":{  
               "type":"string",
               "title":"The Identifier is a unique string that identifies a resource. For software, determine whether the identifier is for a specific version of a piece of software, (per the Force11 Software Citation Principles), or for all versions.",
               "examples":[  
                  "10.5072/example-full"
               ]
            }
         },
         "required":[  
            "identifierType"
         ]
      },
      "creators":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "creatorName":{  
                  "type":"object",
                  "properties":{  
                     "nameType":{  
                        "type":"string",
                        "enum":[  
                           "Organisational",
                           "Personal"
                        ],
                        "title":"The type of name.",
                        "default":"Personal",
                        "examples":[  
                           "Personal"
                        ]
                     },
                     "value":{  
                        "type":"string",
                        "title":"The full name of the creator.",
                        "examples":[  
                           "Miller, Elizabeth"
                        ]
                     },
                     "lang":{  
                        "type":"string",
                        "title":"The language of the Creator.",
                        "examples":[  
                           "en-US"
                        ]
                     }
                  }
               },
               "givenName":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"The personal or first name of the creator.",
                        "examples":[  
                           "Elizabeth"
                        ]
                     }
                  }
               },
               "familyName":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"The surname or last name of the creator.",
                        "examples":[  
                           "Miller"
                        ]
                     }
                  }
               },
               "nameIdentifier":{  
                  "type":"object",
                  "properties":{  
                     "schemeURI":{  
                        "type":"string",
                        "title":"The URI of the name identifier schema",
                        "examples":[  
                           "http://orcid.org/"
                        ]
                     },
                     "nameIdentifierScheme":{  
                        "type":"string",
                        "title":"The name of the name identifier schema.",
                        "examples":[  
                           "ORCID"
                        ]
                     },
                     "value":{  
                        "type":"string",
                        "title":"Uniquely identifies an individual or legal entity, according to various schemas.",
                        "examples":[  
                           "0000-0001-5000-0007"
                        ]
                     }
                  },
                  "required":[  
                     "nameIdentifierScheme"
                  ]
               },
               "affiliation":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"The organisational or institutional affiliation of the Creator.",
                        "examples":[  
                           "DataCite"
                        ]
                     },
                     "affiliationIdentifier":{  
                        "type":"string",
                        "title":"The identifier of the affiliation."
                     },
                     "affiliationIdentifierScheme":{  
                        "type":"string",
                        "title":"The name of the schema of the affiliation identifier."
                     },
                     "schemeURI":{  
                        "type":"string",
                        "title":"The URI of the affiliation identifier scheme."
                     }
                  }
               }
            },
            "required":[  
               "creatorName"
            ]
         }
      },
      "titles":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "lang":{  
                  "type":"string",
                  "title":"The language of Title",
                  "examples":[  
                     "en-US"
                  ]
               },
               "value":{  
                  "type":"string",
                  "title":"A name or title by which a resource is known. May be the title of a dataset or the name of a piece of software.",
                  "examples":[  
                     "Full DataCite XML Example"
                  ]
               },
               "titleType":{  
                  "type":"string",
                  "title":"The type of Title",
                  "enum":[  
                     "AlternativeTitle",
                     "Subtitle",
                     "TranslatedTitle",
                     "Other"
                  ]
               }
            },
            "required":[  
               "value"
            ]
         }
      },
      "publisher":{  
         "type":"object",
         "properties":{  
            "lang":{  
               "type":"string",
               "title":"The language of the publisher.",
               "examples":[  
                  "en-US"
               ]
            },
            "value":{  
               "type":"string",
               "title":"The name of the entity that holds, archives, publishes prints, distributes, releases, issues, or produces the resource. This property will be used to formulate the citation, so consider the prominence of the role. For software, use Publisher for the code repository. If there is an entity other than a code repository, that 'holds, archives, publishes, prints, distributes, releases, issues, or produces' the code, use the property Contributor/contributorType/ hostingInstitution for the code repository.",
               "examples":[  
                  "DataCite"
               ]
            }
         }
      },
      "publicationYear":{  
         "type":"number",
         "title":"The year when the data was or will be made publicly available. In the case of resources such as software or dynamic data where there may be multiple releases in one year, include the Date/dateType/ dateInformation property and sub-properties to provide more information about the publication or release date details.",
         "examples":[  
            "2014"
         ]
      },
      "subjects":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "lang":{  
                  "type":"string",
                  "title":"The language of the Subject.",
                  "examples":[  
                     "en-US"
                  ]
               },
               "schemeURI":{  
                  "type":"string",
                  "format":"uri",
                  "title":"The URI of the subject identifier scheme.",
                  "examples":[  
                     "http://dewey.info/"
                  ]
               },
               "subjectScheme":{  
                  "type":"string",
                  "title":"The name of the subject scheme or classification code or authority if one is used.",
                  "examples":[  
                     "dewey"
                  ]
               },
               "value":{  
                  "type":"string",
                  "title":"Subject, keyword, classification code, or key phrase describing the resource.",
                  "examples":[  
                     "000 computer science"
                  ]
               },
               "valueURI":{  
                  "type":"string",
                  "title":"The URI of the subject term."
               }
            }
         }
      },
      "contributors":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "contributorType":{  
                  "type":"string",
                  "enum":[  
                     "ContactPerson",
                     "DataCollector",
                     "DataCurator",
                     "DataManager",
                     "Distributor",
                     "Editor",
                     "HostingInstitution",
                     "Producer",
                     "ProjectLeader",
                     "ProjectManager",
                     "ProjectMember",
                     "RegistrationAgency",
                     "RegistrationAuthority",
                     "RelatedPerson",
                     "Researcher",
                     "ResearchGroup",
                     "RightsHolder",
                     "Sponsor",
                     "Supervisor",
                     "WorkPackageLeader",
                     "Other"
                  ],
                  "title":"The type of contributor of the resource.",
                  "examples":[  
                     "ProjectLeader"
                  ]
               },
               "contributorName":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"The full name of the contributor.",
                        "examples":[  
                           "Starr, Joan"
                        ]
                     },
                     "nameType":{  
                        "type":"string",
                        "enum":[  
                           "Organisational",
                           "Personal"
                        ],
                        "title":"The type of name.",
                        "examples":[  
                           "Personal"
                        ]
                     },
                     "lang":{  
                        "type":"string",
                        "title":"The language of the Contributor.",
                        "examples":[  
                           "en-US"
                        ]
                     }
                  }
               },
               "givenName":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"The personal or first name of the contributor.",
                        "examples":[  
                           "Joan"
                        ]
                     }
                  }
               },
               "familyName":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"The surname or last name of the contributor.",
                        "examples":[  
                           "Starr"
                        ]
                     }
                  }
               },
               "nameIdentifier":{  
                  "type":"object",
                  "properties":{  
                     "schemeURI":{  
                        "type":"string",
                        "format":"uri",
                        "title":"The URI of the name identifier scheme.",
                        "examples":[  
                           "http://orcid.org/"
                        ]
                     },
                     "nameIdentifierScheme":{  
                        "type":"string",
                        "title":"The name of the name identifier scheme.",
                        "examples":[  
                           "ORCID"
                        ]
                     },
                     "value":{  
                        "type":"string",
                        "title":"Uniquely identifies an individual or legal entity, according to various schemes.",
                        "examples":[  
                           "0000-0002-7285-027X"
                        ]
                     }
                  },
                  "required":[  
                     "nameIdentifierScheme"
                  ]
               },
               "affiliation":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"The organisational or institutional affiliation of the Contributor.",
                        "examples":[  
                           "DataCite"
                        ]
                     },
                     "affiliationIdentifier":{  
                        "type":"string",
                        "title":"The identifier of the affiliation."
                     },
                     "affiliationIdentifierScheme":{  
                        "type":"string",
                        "title":"The name of the schema of the affiliation identifier."
                     },
                     "schemeURI":{  
                        "type":"string",
                        "title":"The URI of the affiliation identifier scheme."
                     }
                  }
               }
            },
            "required":[  
               "contributorType",
               "contributorName"
            ]
         }
      },
      "dates":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "dateType":{  
                  "type":"string",
                  "enum":[  
                     "Accepted",
                     "Available",
                     "Copyrighted",
                     "Collected",
                     "Created",
                     "Issued",
                     "Submitted",
                     "Updated",
                     "Valid",
                     "Other",
                     "Withdrawn"
                  ],
                  "title":"The type of date.",
                  "examples":[  
                     "Updated"
                  ]
               },
               "dateInformation":{  
                  "type":"string",
                  "title":"Specific information about the date, if appropriate.",
                  "examples":[  
                     "Updated with 4.1 properties"
                  ]
               },
               "value":{  
                  "type":"string",
                  "minLength":2,
                  "pattern":"^([0-9]{4}(-[0-9]{2}(-[0-9]{2}(T[0-9]{2}:[0-9]{2}(:[0-9]{2}(\\.[0-9]+)?)?([+-][0-9]{2}:[0-9]{2}|Z))?)?)?)?(\\/([0-9]{4}(-[0-9]{2}(-[0-9]{2}(T[0-9]{2}:[0-9]{2}(:[0-9]{2}(\\.[0-9]+)?)?([+-][0-9]{2}:[0-9]{2}|Z))?)?)?)?)?$",
                  "title":"Different dates relevant to the work.",
                  "examples":[  
                     "2017-09-13"
                  ]
               }
            }
         }
      },
      "language":{  
         "type":"string",
         "title":"The primary language of the resource.",
         "examples":[  
            "en-US"
         ]
      },
      "resourceType":{  
         "type":"object",
         "properties":{  
            "resourceTypeGeneral":{  
               "type":"string",
               "title":"The general type of a resource.",
               "examples":[  
                  "Software"
               ]
            },
            "value":{  
               "type":"string",
               "title":"A description of the resource.",
               "examples":[  
                  "XML"
               ]
            }
         }
      },
      "alternateIdentifiers":{  
         "type":"array",
         "items":{  
            "properties":{  
               "alternateIdentifierType":{  
                  "type":"string",
                  "title":"The type of the AlternateIdentifier.",
                  "examples":[  
                     "URL"
                  ]
               },
               "value":{  
                  "type":"string",
                  "title":"An identifier or identifiers other than the primary Identifier applied to the resource being registered. This may be any alphanumeric string which is unique within its domain of issue. May be used for local identifiers. AlternateIdentifier should be used for another identifier of the same instance (same location, same file).",
                  "examples":[  
                     "https://schema.datacite.org/meta/kernel-4.1/example/datacite-example-full-v4.1.xml"
                  ]
               }
            },
            "required":[  
               "alternateIdentifierType"
            ]
         }
      },
      "relatedIdentifiers":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "relatedIdentifierType":{  
                  "type":"string",
                  "enum":[  
                     "ARK",
                     "arXiv",
                     "bibcode",
                     "DOI",
                     "EAN13",
                     "EISSN",
                     "Handle",
                     "IGSN",
                     "ISBN",
                     "ISSN",
                     "ISTC",
                     "LISSN",
                     "LSID",
                     "PMID",
                     "PURL",
                     "UPC",
                     "URL",
                     "URN",
                     "w3id"
                  ],
                  "title":"The type of the RelatedIdentifier.",
                  "examples":[  
                     "URL"
                  ]
               },
               "relationType":{  
                  "type":"string",
                  "enum":[  
                     "IsCitedBy",
                     "Cites",
                     "IsSupplementTo",
                     "IsSupplementedBy",
                     "IsContinuedBy",
                     "Continues",
                     "IsDescribed by",
                     "Describes",
                     "HasMetadata",
                     "IsMetadataFor",
                     "HasVersion",
                     "IsVersionOf",
                     "IsNewVersionOf",
                     "IsPreviousVersionOf",
                     "IsPartOf",
                     "HasPart",
                     "IsReferencedBy",
                     "References",
                     "IsDocumentedBy",
                     "Documents",
                     "IsCompiledBy",
                     "Compiles",
                     "IsVariantFormOf",
                     "IsOriginalFormOf",
                     "IsIdenticalTo",
                     "IsReviewedBy",
                     "Reviews",
                     "IsDerivedFrom",
                     "IsSourceOf",
                     "IsRequiredBy",
                     "Requires",
                     "IsObsoletedBy",
                     "Obsoletes"
                  ],
                  "title":"Description of the relationship of the resource being registered (A) and the related resource (B).",
                  "examples":[  
                     "HasMetadata"
                  ]
               },
               "relatedMetadataScheme":{  
                  "type":"string",
                  "title":"The name of the scheme.",
                  "examples":[  
                     "citeproc+json"
                  ]
               },
               "schemeURI":{  
                  "type":"string",
                  "format":"uri",
                  "title":"The URI of the relatedMetadataScheme.",
                  "examples":[  
                     "https://github.com/citation-style-language/schema/raw/master/csl-data.json"
                  ]
               },
               "schemeType":{  
                  "type":"string",
                  "title":"The type of the relatedMetadataScheme, linked with the schemeURI.",
                  "examples":[  
                     "XSD"
                  ]
               },
               "value":{  
                  "type":"string",
                  "title":"Identifiers of related resources. These must be globally unique identifiers",
                  "examples":[  
                     "https://data.datacite.org/application/citeproc+json/10.5072/example-full"
                  ]
               }
            },
            "required":[  
               "relatedIdentifierType",
               "relationType"
            ]
         }
      },
      "sizes":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "size":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"Size (e.g. bytes, pages, inches, etc.) or duration (extent), e.g. hours, minutes, days, etc., of a resource.",
                        "examples":[  
                           "4 kB"
                        ]
                     }
                  }
               }
            }
         }
      },
      "formats":{  
         "type":"array",
         "title":"Technical format of the resource.",
         "items":{  
            "type":"string"
         }
      },
      "version":{  
         "type":"string",
         "title":"The version number of the resource",
         "examples":[  
            "4.1"
         ]
      },
      "rightsList":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "lang":{  
                  "type":"string",
                  "title":"The language of the right's description.",
                  "examples":[  
                     "en-US"
                  ]
               },
               "rightsURI":{  
                  "type":"string",
                  "format":"uri",
                  "title":"The URI of the license.",
                  "examples":[  
                     "http://creativecommons.org/publicdomain/zero/1.0/"
                  ]
               },
               "value":{  
                  "type":"string",
                  "title":"Any rights information for this resource. The property may be repeated to record complex rights characteristics.",
                  "examples":[  
                     "Creative Commons, Attribution 3.0 Germany"
                  ]
               },
               "rightsIdentifier":{  
                  "type":"string",
                  "title":"A short, standardized version of the license name.",
                  "examples":[  
                     "CC-BY-3.0"
                  ]
               },
               "rightsIdentifierScheme":{  
                  "type":"string",
                  "title":"The name of the scheme.",
                  "examples":[  
                     "SPDX"
                  ]
               },
               "schemeURI":{  
                  "type":"string",
                  "title":"The URI of the funder identifier scheme.",
                  "examples":[  
                     "http://doi.org/"
                  ]
               }
            }
         }
      },
      "descriptions":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "lang":{  
                  "type":"string",
                  "title":"The language of the description.",
                  "examples":[  
                     "en-US"
                  ]
               },
               "descriptionType":{  
                  "type":"string",
                  "enum":[  
                     "Abstract",
                     "Methods",
                     "SeriesInformation",
                     "TableOfContents",
                     "TechnicalInfo",
                     "Other"
                  ],
                  "title":"The type of the Description.",
                  "examples":[  
                     "Abstract"
                  ]
               },
               "value":{  
                  "type":"string",
                  "title":"All additional information that does not fit in any of the other categories. May be used for technical information.",
                  "examples":[  
                     "XML example of all DataCite Metadata Schema v4.1 properties."
                  ]
               }
            },
            "required":[  
               "descriptionType"
            ]
         }
      },
      "geoLocations":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "geoLocationPlace":{  
                  "type":"object",
                  "properties":{  
                     "value":{  
                        "type":"string",
                        "title":"Description of a geographic location.",
                        "examples":[  
                           "Atlantic Ocean"
                        ]
                     }
                  }
               },
               "geoLocationPoint":{  
                  "type":"object",
                  "properties":{  
                     "pointLongitude":{  
                        "type":"number",
                        "minimum":-180,
                        "maximum":180,
                        "title":"Longitudinal dimension of point.",
                        "examples":[  
                           "-67.302"
                        ]
                     },
                     "pointLatitude":{  
                        "type":"number",
                        "minimum":-90,
                        "maximum":90,
                        "title":"Latitudinal dimension of point.",
                        "examples":[  
                           "31.233"
                        ]
                     }
                  },
                  "required":[  
                     "pointLatitude",
                     "pointLongitude"
                  ]
               },
               "geoLocationBox":{  
                  "type":"object",
                  "properties":{  
                     "westBoundLongitude":{  
                        "type":"number",
                        "minimum":-180,
                        "maximum":180,
                        "title":"Western longitudinal dimension of box.",
                        "examples":[  
                           "-71.032"
                        ]
                     },
                     "eastBoundLongitude":{  
                        "type":"number",
                        "minimum":-180,
                        "maximum":180,
                        "title":"Eastern longitudinal dimension of box.",
                        "examples":[  
                           "-68.211"
                        ]
                     },
                     "southBoundLatitude":{  
                        "type":"number",
                        "minimum":-90,
                        "maximum":90,
                        "title":"Southern latitudinal dimension of box.",
                        "examples":[  
                           "41.090"
                        ]
                     },
                     "northBoundLatitude":{  
                        "type":"number",
                        "minimum":-90,
                        "maximum":90,
                        "title":"Northern latitudinal dimension of box.",
                        "examples":[  
                           "42.893"
                        ]
                     }
                  },
                  "required":[  
                     "westBoundLongitude",
                     "eastBoundLongitude",
                     "northBoundLongitude",
                     "southBoundLongitude"
                  ]
               },
               "geoLocationPolygon":{  
                  "type":"object",
                  "properties":{  
                     "polygonPoint":{  
                        "type":"array",
                        "minItems":4,
                        "items":{  
                           "type":"object",
                           "properties":{  
                              "pointLongitude":{  
                                 "type":"number",
                                 "minimum":-180,
                                 "maximum":180,
                                 "title":"Longitudinal dimension of point.",
                                 "examples":[  
                                    "-67.302"
                                 ]
                              },
                              "pointLatitude":{  
                                 "type":"number",
                                 "minimum":-90,
                                 "maximum":90,
                                 "title":"Latitudinal dimension of point.",
                                 "examples":[  
                                    "31.233"
                                 ]
                              }
                           }
                        }
                     }
                  }
               },
               "inPolygonPoint":{  
                  "type":"object",
                  "properties":{  
                     "pointLongitude":{  
                        "type":"number",
                        "minimum":-180,
                        "maximum":180,
                        "title":"Longitudinal dimension of point.",
                        "examples":[  
                           "-67.302"
                        ]
                     },
                     "pointLatitude":{  
                        "type":"number",
                        "minimum":-90,
                        "maximum":90,
                        "title":"Latitudinal dimension of point.",
                        "examples":[  
                           "31.233"
                        ]
                     }
                  }
               }
            }
         }
      },
      "fundingReferences":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "fundingReference":{  
                  "type":"object",
                  "properties":{  
                     "funderName":{  
                        "type":"string",
                        "title":"Name of the funding provider.",
                        "examples":[  
                           "National Science Foundation"
                        ]
                     }
                  },
                  "funderIdentifier":{  
                     "type":"object",
                     "properties":{  
                        "funderIdentifierType":{  
                           "type":"string",
                           "enum":[  
                              "ISNI",
                              "GRID",
                              "Crossref Funder",
                              "Other",
                              "ROR"
                           ],
                           "title":"The type of the funderIdentifier.",
                           "examples":[  
                              "Crossref Funder ID"
                           ]
                        },
                        "value":{  
                           "type":"string",
                           "title":"Uniquely identifies a funding entity, according to various types.",
                           "examples":[  
                              "https://doi.org/10.13039/100000001"
                           ]
                        },
                        "schemeURI":{  
                           "type":"string",
                           "title":"The URI of the funder identifier scheme.",
                           "examples":[  
                              "http://doi.org/"
                           ]
                        }
                     }
                  },
                  "awardNumber":{  
                     "type":"object",
                     "properties":{  
                        "value":{  
                           "type":"string",
                           "title":"The code assigned by the funder to a sponsored award (grant).",
                           "examples":[  
                              "CBET-106"
                           ]
                        },
                        "awardURI":{  
                           "type":"string",
                           "format":"uri",
                           "title":"The URI leading to a page provided by the funder for more information about the award (grant)."
                        }
                     }
                  },
                  "awardTitle":{  
                     "type":"object",
                     "properties":{  
                        "value":{  
                           "type":"string",
                           "title":"The human readable title or name of the award (grant). ",
                           "examples":[  
                              "Full DataCite XML Example"
                           ]
                        }
                     }
                  }
               }
            }
         }
      },
      "webLinks":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "webLinkName":{  
                  "type":"string",
                  "title":"String value denoting the name of the link (sth that the user would see during browsing).",
                  "examples":[  
                     "Fishing quotas"
                  ]
               },
               "webLinkURI":{  
                  "type":"string",
                  "title":"The (access) URI of the resource",
                  "examples":[  
                     "http://gerdi.org/randomlink/123.csv"
                  ]
               },
               "webLinkType":{  
                  "type":"string",
                  "enum":[  
                     "ViewURL",
                     "SourceURL",
                     "ProviderLogoURL",
                     "ThumbnailURL",
                     "Related"
                  ],
                  "title":"The type of the weblink.",
                  "examples":[  
                     "ViewURL"
                  ]
               }
            },
            "required":[  
               "webLinkURI"
            ]
         }
      },
      "repositoryIdentifier":{  
         "type":"string",
         "title":"A unique human readable string that identifies the source repository."
      },
      "researchDataList":{  
         "type":"array",
         "items":{  
            "type":"object",
            "properties":{  
               "researchDataIdentifier":{  
                  "title":"A downloadable file from the source repository.",
                  "type":"string"
               },
               "researchDataURL":{  
                  "title":"The URL that is points to the download of the file.",
                  "type":"string",
                  "format":"uri"
               },
               "researchDataLabel":{  
                  "title":"A human readable name of the file.",
