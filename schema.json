{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "definitions": {},
  "properties": {
    "identifier": {

      "type": "object",
      "properties": {
        "identifierType": {
          "type": "string",
          "enum": ["DOI"],
          "title": "The type of the identifier.",
          "default": "DOI",
          "examples": [
            "DOI"
          ]
        },
        "value": {
          "type": "string",
          "title": "The document's identifier.",
          "default": "",
          "examples": [
            "10.5072/example-full"
          ]
        }
      },
      "required": ["identifierType"]
    },
    "creators": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "creatorName": {
            "type": "object",
            "properties": {
              "nameType": {
                "type": "string",
                "enum": ["Organisational", "Personal"],
                "title": "The Nametype Schema ",
                "default": "",
                "examples": [
                  "Personal"
                ]
              },
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "Miller, Elizabeth"
                ]
              }
            }
          },
          "givenName": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "Elizabeth"
                ]
              }
            }
          },
          "familyName": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "Miller"
                ]
              }
            }
          },
          "nameIdentifier": {
            "type": "object",
            "properties": {
              "schemeURI": {
                "type": "string",
                "title": "The Schemeuri Schema ",
                "default": "",
                "examples": [
                  "http://orcid.org/"
                ]
              },
              "nameIdentifierScheme": {
                "type": "string",
                "title": "The Nameidentifierscheme Schema ",
                "default": "",
                "examples": [
                  "ORCID"
                ]
              },
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "0000-0001-5000-0007"
                ]
              }
            },
            "required": ["nameIdentifierScheme"]
          },
          "affiliation": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "DataCite"
                ]
              }
            }
          }
        },
        "required": ["creatorName"]
      }
    },
    "titles": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "lang": {
            "type": "string",
            "title": "The Lang Schema ",
            "default": "",
            "examples": [
              "en-US"
            ]
          },
          "value": {
            "type": "string",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "Full DataCite XML Example"
            ]
          },
          "titleType": {
            "type": "string",
            "enum": ["AlternativeTitle", "Subtitle", "TranslatedTitle", "Other"]
          }
        },
        "required": ["value"]
      }
    },
    "publisher": {
      "type": "string",
      "title": "The Value Schema ",
      "default": "",
      "examples": [
        "DataCite"
      ]
    },
    "publicationYear": {
      "type": "number",
      "title": "The Value Schema ",
      "default": "",
      "examples": [
        "2014"
      ]
    },
    "subjects": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "lang": {
            "type": "string",
            "title": "The Lang Schema ",
            "default": "",
            "examples": [
              "en-US"
            ]
          },
          "schemeURI": {
            "type": "string",
            "format": "uri",
            "title": "The Schemeuri Schema ",
            "default": "",
            "examples": [
              "http://dewey.info/"
            ]
          },
          "subjectScheme": {
            "type": "string",
            "title": "The Subjectscheme Schema ",
            "default": "",
            "examples": [
              "dewey"
            ]
          },
          "value": {
            "type": "string",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "000 computer science"
            ]
          }
        }
      }
    },
    "contributors": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "contributorType": {
            "type": "string",
            "enum": ["ContactPerson", "DataCollector", "DataCurator", "DataManager", "Distributor", "Editor", "HostingInstitution", "Producer", "ProjectLeader", "ProjectManager", "ProjectMember", "RegistrationAgency", "RegistrationAuthority", "RelatedPerson", "Researcher", "ResearchGroup", "RightsHolder", "Sponsor", "Supervisor", "WorkPackageLeader", "Other"],
            "title": "The Contributortype Schema ",
            "default": "",
            "examples": [
              "ProjectLeader"
            ]
          },
          "contributorName": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "Starr, Joan"
                ]
              }
            }
          },
          "nameType": {
            "type": "string",
            "enum": ["Organisational", "Personal"],
            "title": "The Nametype Schema ",
            "default": "",
            "examples": [
              "Personal"
            ]
          },
          "givenName": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "Joan"
                ]
              }
            }
          },
          "familyName": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "Starr"
                ]
              }
            }
          },
          "nameIdentifier": {
            "type": "object",
            "properties": {
              "schemeURI": {
                "type": "string",
                "format": "uri",
                "title": "The Schemeuri Schema ",
                "default": "",
                "examples": [
                  "http://orcid.org/"
                ]
              },
              "nameIdentifierScheme": {
                "type": "string",
                "title": "The Nameidentifierscheme Schema ",
                "default": "",
                "examples": [
                  "ORCID"
                ]
              },
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "0000-0002-7285-027X"
                ]
              }
            },
            "required": ["nameIdentifierScheme"]
          },
          "affiliation": {
            "type": "array",
            "items": {
              "type": "string",
              "title": "The Value Schema ",
              "default": "",
              "examples": [
                "California Digital Library"
              ]
            }
          }
        },
        "required": ["contributorType", "contributorName"]
      }
    },
    "dates": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "dateType": {
            "type": "string",
            "enum": ["Accepted","Available","Copyrighted","Collected","Created","Issued","Submitted","Updated","Valid","Other"],
            "title": "The Datetype Schema ",
            "default": "",
            "examples": [
              "Updated"
            ]
          },
          "dateInformation": {
            "type": "string",
            "title": "The Dateinformation Schema ",
            "default": "",
            "examples": [
              "Updated with 4.1 properties"
            ]
          },
          "value": {
            "type": "string",
            "pattern": "^([0-9]{4}(-[0-9]{2}(-[0-9]{2}(T[0-9]{2}:[0-9]{2}(:[0-9]{2}(\\.[0-9]+)?)?([+-][0-9]{2}:[0-9]{2}|Z))?)?)?)?(\\/([0-9]{4}(-[0-9]{2}(-[0-9]{2}(T[0-9]{2}:[0-9]{2}(:[0-9]{2}(\\.[0-9]+)?)?([+-][0-9]{2}:[0-9]{2}|Z))?)?)?)?)?$",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "2017-09-13"
            ]
          }
        }
      }
    },
    "language": {
      "type": "string",
      "title": "The Value Schema ",
      "default": "",
      "examples": [
        "en-US"
      ]
    },
    "resourceType": {
      "type": "object",
      "properties": {
        "resourceTypeGeneral": {
          "type": "string",
          "title": "The Resourcetypegeneral Schema ",
          "default": "",
          "examples": [
            "Software"
          ]
        },
        "value": {
          "type": "string",
          "title": "The Value Schema ",
          "default": "",
          "examples": [
            "XML"
          ]
        }
      }
    },
    "alternateIdentifiers": {
      "type": "array",
      "items": {
        "properties": {
          "alternateIdentifierType": {
            "type": "string",
            "title": "The Alternateidentifiertype Schema ",
            "default": "",
            "examples": [
              "URL"
            ]
          },
          "value": {
            "type": "string",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "https://schema.datacite.org/meta/kernel-4.1/example/datacite-example-full-v4.1.xml"
            ]
          }
        },
        "required": ["alternateIdentifierType"]
      }
    },
    "relatedIdentifiers": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "relatedIdentifierType": {
            "type": "string",
            "enum": ["ARK","arXiv","bibcode","DOI","EAN13","EISSN","Handle","IGSN","ISBN","ISSN","ISTC","LISSN","LSID","PMID","PURL","UPC","URL","URN"],
            "title": "The Relatedidentifiertype Schema ",
            "default": "",
            "examples": [
              "URL"
            ]
          },
          "relationType": {
            "type": "string",
            "enum": ["IsCitedBy","Cites","IsSupplementTo","IsSupplementedBy","IsContinuedBy","Continues","IsDescribed by","Describes","HasMetadata","IsMetadataFor","HasVersion","IsVersionOf","IsNewVersionOf","IsPreviousVersionOf","IsPartOf","HasPart","IsReferencedBy","References","IsDocumentedBy","Documents","IsCompiledBy","Compiles","IsVariantFormOf","IsOriginalFormOf","IsIdenticalTo","IsReviewedBy","Reviews","IsDerivedFrom","IsSourceOf","IsRequiredBy","Requires"],
            "title": "The Relationtype Schema ",
            "default": "",
            "examples": [
              "HasMetadata"
            ]
          },
          "relatedMetadataScheme": {
            "type": "string",
            "title": "The Relatedmetadatascheme Schema ",
            "default": "",
            "examples": [
              "citeproc+json"
            ]
          },
          "schemeURI": {
            "type": "string",
            "format": "uri",
            "title": "The Schemeuri Schema ",
            "default": "",
            "examples": [
              "https://github.com/citation-style-language/schema/raw/master/csl-data.json"
            ]
          },
          "value": {
            "type": "string",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "https://data.datacite.org/application/citeproc+json/10.5072/example-full"
            ]
          }
        },
        "required": ["relatedIdentifierType", "relationType"]
      }
    },
    "sizes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "size": {
            "type": "object",
            "properties": {
              "value": {
                "type": "string",
                "title": "The Value Schema ",
                "default": "",
                "examples": [
                  "4 kB"
                ]
              }
            }
          }
        }
      }
    },
    "formats": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "version": {
      "type": "string",
      "title": "The Value Schema ",
      "default": "",
      "examples": [
        "4.1"
      ]
    },
    "rightsList": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "lang": {
            "type": "string",
            "title": "The Lang Schema ",
            "default": "",
            "examples": [
              "en-US"
            ]
          },
          "rightsURI": {
            "type": "string",
            "format": "uri",
            "title": "The Rightsuri Schema ",
            "default": "",
            "examples": [
              "http://creativecommons.org/publicdomain/zero/1.0/"
            ]
          },
          "value": {
            "type": "string",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "CC0 1.0 Universal"
            ]
          }
        }
      }
    },
    "descriptions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "lang": {
            "type": "string",
            "title": "The Lang Schema ",
            "default": "",
            "examples": [
              "en-US"
            ]
          },
          "descriptionType": {
            "type": "string",
            "enum": ["Abstract","Methods","SeriesInformation","TableOfContents","TechnicalInfo","Other"],
            "title": "The Descriptiontype Schema ",
            "default": "",
            "examples": [
              "Abstract"
            ]
          },
          "value": {
            "type": "string",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "XML example of all DataCite Metadata Schema v4.1 properties."
            ]
          }
        },
        "required": ["descriptionType"]
      }
    },
    "geoLocations": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "geoLocation": {
            "type": "object",
            "properties": {
              "geoLocationPlace": {
                "type": "object",
                "properties": {
                  "value": {
                    "type": "string",
                    "title": "The Value Schema ",
                    "default": "",
                    "examples": [
                      "Atlantic Ocean"
                    ]
                  }
                }
              },
              "geoLocationPoint": {
                "type": "object",
                "properties": {
                  "pointLongitude": {
                    "type": "object",
                    "properties": {
                      "value": {
                        "type": "string",
                        "title": "The Value Schema ",
                        "default": "",
                        "examples": [
                          "-67.302"
                        ]
                      }
                    }
                  },
                  "pointLatitude": {
                    "type": "object",
                    "properties": {
                      "value": {
                        "type": "string",
                        "title": "The Value Schema ",
                        "default": "",
                        "examples": [
                          "31.233"
                        ]
                      }
                    }
                  }
                }
              },
              "geoLocationBox": {
                "type": "object",
                "properties": {
                  "westBoundLongitude": {
                    "type": "object",
                    "properties": {
                      "value": {
                        "type": "string",
                        "title": "The Value Schema ",
                        "default": "",
                        "examples": [
                          "-71.032"
                        ]
                      }
                    }
                  },
                  "eastBoundLongitude": {
                    "type": "object",
                    "properties": {
                      "value": {
                        "type": "string",
                        "title": "The Value Schema ",
                        "default": "",
                        "examples": [
                          "-68.211"
                        ]
                      }
                    }
                  },
                  "southBoundLatitude": {
                    "type": "object",
                    "properties": {
                      "value": {
                        "type": "string",
                        "title": "The Value Schema ",
                        "default": "",
                        "examples": [
                          "41.090"
                        ]
                      }
                    }
                  },
                  "northBoundLatitude": {
                    "type": "object",
                    "properties": {
                      "value": {
                        "type": "string",
                        "title": "The Value Schema ",
                        "default": "",
                        "examples": [
                          "42.893"
                        ]
                      }
                    }
                  }
                }
              },
              "geoLocationPolygon": {
                "type": "object",
                "properties": {
                  "polygonPoint": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "pointLatitude": {
                          "type": "object",
                          "properties": {
                            "value": {
                              "type": "string",
                              "title": "The Value Schema ",
                              "default": "",
                              "examples": [
                                "41.991"
                              ]
                            }
                          }
                        },
                        "pointLongitude": {
                          "type": "object",
                          "properties": {
                            "value": {
                              "type": "string",
                              "title": "The Value Schema ",
                              "default": "",
                              "examples": [
                                "-71.032"
                              ]
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "fundingReferences": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "fundingReference": {
            "type": "object",
            "properties": {
              "funderName": {
                "type": "object",
                "properties": {
                  "value": {
                    "type": "string",
                    "title": "The Value Schema ",
                    "default": "",
                    "examples": [
                      "National Science Foundation"
                    ]
                  }
                }
              },
              "funderIdentifier": {
                "type": "object",
                "properties": {
                  "funderIdentifierType": {
                    "type": "string",
                    "enum": ["ISNI","GRID","Crossref Funder","Other"],
                    "title": "The Funderidentifiertype Schema ",
                    "default": "",
                    "examples": [
                      "Crossref Funder ID"
                    ]
                  },
                  "value": {
                    "type": "string",
                    "title": "The Value Schema ",
                    "default": "",
                    "examples": [
                      "https://doi.org/10.13039/100000001"
                    ]
                  }
                }
              },
              "awardNumber": {
                "type": "object",
                "properties": {
                  "value": {
                    "type": "string",
                    "title": "The Value Schema ",
                    "default": "",
                    "examples": [
                      "CBET-106"
                    ]
                  }
                }
              },
              "awardTitle": {
                "type": "object",
                "properties": {
                  "value": {
                    "type": "string",
                    "title": "The Value Schema ",
                    "default": "",
                    "examples": [
                      "Full DataCite XML Example"
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
    "webLinks": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "webLinkName": {
            "type": "string",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "Fishing quotas"
            ]
          },
          "webLinkURI": {
            "type": "string",
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "http://gerdi.org/randomlink/123.csv"
            ]
          },
          "webLinkType": {
            "type": "string",
            "enum": ["ViewURL", "SourceURL", "ProviderLogoURL", "ThumbnailURL", "Related"],
            "title": "The Value Schema ",
            "default": "",
            "examples": [
              "ViewURL"
            ]
          }
        },
        "required": ["webLinkURI"]
      }
    },
    "repositoryIdentifier": {
      "type": "string",
      "title": "The repository's identifier"
    },
    "researchDataList": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "researchDataIdentifier": {
            "type": "string"
          },
          "researchDataURL": {
            "type": "string",
            "format": "uri"
          },
          "researchDataLabel": {
            "type": "string"
          },
          "researchDataType": {
            "type": "string"
          }
        },
        "required": ["researchDataIdentifier", "researchDataURL"]
      }
    },
    "researchDiscipline": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "discipline": {
            "type": "string"
          },
          "area": {
            "type": "string"
          },
          "category": {
            "type": "string"
          },
          "rnbr": {
            "type": "string",
            "pattern": "^[0-9]{3}(-[0-9]{2})?$"
          }
        }
      }
    }
  },
  "required": ["identifier", "creators", "titles", "publisher", "publicationYear", "resourceType", "repositoryIdentifier", "researchDataList"]
}
